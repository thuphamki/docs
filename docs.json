{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Pricenxt Docs",
  "colors": {
    "primary": "#0077B6",
    "light": "#BAE4FC",
    "dark": "#083447"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Pricenxt concepts",
            "pages": [
              "pricenxt/boards",
              "pricenxt/cards",
              "pricenxt/price-history-chart"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Back to Pricenxt",
          "href": "/",
          "icon": "arrow-left"
        },
        {
          "anchor": "Feature tour",
          "href": "/pricenxt/boards",
          "icon": "sparkles"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "/docs"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact support",
        "href": "mailto:support@pricenxt.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Open Pricenxt",
      "href": "/"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/pricenxt",
      "github": "https://github.com/devkigroup/pricenxt",
      "linkedin": "https://www.linkedin.com/company/pricenxt"
    }
  }
}
